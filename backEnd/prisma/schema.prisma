generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model lancamento {
  id        Int      @id @default(autoincrement())
  descricao String
  data      DateTime
  valor     Decimal  @db.Decimal(10,2)
  tipo_lancamento      String
  createdAt DateTime @default(now())
}
